}
  .money-box{
    position: fixed;
    left: 0;
    top: 1084rpx;
    right: 0;
    z-index: 9999;
    width: 100%;
    height: 100rpx;
    background-color: #00aa99;
    display: flex;
    flex-direction: column;
  }
  .money{
    position: relative;
    font-size: 48rpx;
    color: rgb(7, 7, 7);
    text-align: center;
    margin-top: 20rpx;
  }
  